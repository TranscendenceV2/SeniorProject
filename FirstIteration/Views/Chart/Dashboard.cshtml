@{
    Layout = null;
}
@model FirstIteration.Models.transcendenceEntities
<!doctype html>
<!--   Core JS Files   -->
<script src="../../Scripts/jquery-1.10.2.js" type="text/javascript"></script>
<script src="../../Scripts/bootstrap.min.js" type="text/javascript"></script>
<!--  Checkbox, Radio & Switch Plugins -->
<script src="../../Scripts/bootstrap-checkbox-radio-switch.js"></script>
<script src="../../Scripts/LoadLists.js"></script>
<script>
        $(document).ready(function () {
            $("#loadpartial").load('/Chart/_FundingSourceDropDowns');
        });
</script>
<!--  Charts Plugin -->
<script src="http://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/exporting.js"></script>
<!--  Notifications Plugin    -->
<script src="../../Scripts/bootstrap-notify.js"></script>
<!--  Google Maps Plugin    -->
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>
<!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
<script src="../../Scripts/light-bootstrap-dashboard.js"></script>
<!-- Light Bootstrap Table DEMO methods, don't include it in your project! -->
<script src="../../Scripts/demo.js"></script>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/png" href="../../img/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Clay Behavioral Health Revenue Dashboard</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <!-- Bootstrap core CSS     -->
    <link href="../../content/bootstrap.min.css" rel="stylesheet" />
    <!-- Animation library for notifications   -->
    <link href="../../content/animate.min.css" rel="stylesheet" />
    <!--  Light Bootstrap Table core CSS    -->
    <link href="../../content/light-bootstrap-dashboard.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="../../content/pe-icon-7-stroke.css" rel="stylesheet" />
</head>
<body>
    <div class="wrapper">
        <div class="sidebar" data-color="blue">
            <!--

                Tip 1: you can change the color of the sidebar using: data-color="blue | azure | green | orange | red | purple"
                Tip 2: you can also add an image using data-image tag

            -->
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a class="simple-text">
                        Data Sort Selection
                    </a>
                </div>                
                <div class="btn-group" style="border-radius: 12px; position: relative; left: 5%; top: 10px" id="filterDay" data-toggle="buttons">
                    <label class="btn btn-primary active">
                        <input type="radio" name="options" id="option1" autocomplete="off" checked="checked" value="1"> Funding Source
                    </label>
                    <label class="btn btn-primary">
                        <input type="radio" name="options" id="option2" autocomplete="off" value="2"> Employee
                    </label>
                </div>
                <div id="loadpartial">                
                </div>
            </div>
        </div>
        <div class="main-panel">
            <nav class="navbar navbar-default navbar-fixed">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">Clay Behavioral Health Revenue Dashboard</a>
                    </div>
                    <div class="collapse navbar-collapse">
                        
                        <ul class="nav navbar-nav navbar-right">
                           
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    Upload
                                    <b class="caret"></b>
                                </a>
                                <ul class="dropdown-menu" id="upload-list">
                                    <li id="Transactions"><a href="#">Transactions</a></li>
                                    <li id="Departments"><a href="#">Departments</a></li>
                                    <li id="Staff"><a href="#">Staff</a></li>
                                </ul>
                            </li>
                            
                        </ul>
                    </div>
                </div>
            </nav>

            <div class="content">
                <div class="container-fluid">
                    <!--<div class="row">
                        <div class="col-md-4">
                            <div class="card">
                                <div class="header">
                                    <h4 class="title">Data Statistics</h4>
                                </div>
                                <div class="content">
                                   
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card">
                                <div class="header">
                                    <h4 class="title">Data Statistics</h4>
                                </div>
                                <div class="content">
                                                                  
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card">
                                <div class="content">
                                                                       
                                </div>
                            </div>
                        </div>
                    </div>-->
                    <div class="row">
                        <div class="col-md-6">
                            <div class="card">                                
                                <div class="content">   
                                    @{ Html.RenderPartial("_PieChart");}                
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="card">
                                <div class="content">
                                    @{ Html.RenderPartial("_BarChart");}                               
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-8 col-md-offset-2">
                            <div class="card">
                                <div class="content">
                                    @{ Html.RenderPartial("_LineChart");}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <footer class="footer">
                <div class="container-fluid">      
                    <p class="copyright pull-right">
                        &copy; 2016 Transcendence, made to better Clay Behavioral Health
                    </p>
                </div>
            </footer>
        </div>
    </div>

    <div id="modal-wrapper">

    </div>

    <script src="../../Scripts/bootstrap-filestyle.min.js" type="text/javascript"></script>
    <script src="../../Scripts/Upload.js" type="text/javascript"></script>
</body>


</html>

